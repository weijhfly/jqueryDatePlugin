    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-banner": "^0.2.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-uglify": "^6.0.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.2",
