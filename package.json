{
  "name": "jquery-date",
  "version": "2.0.1",
  "description": "jquery-date 多格式、功能强大的移动端日期选择插件",
  "main": "dist/jdate.min.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/jdate.config.js --mode development",
    "build": "cross-env rollup -c",
    "wpbuild": "cross-env NODE_ENV=production webpack --config config/jdate.config.js --mode production",
    "start": "npm run wpbuild && npm run build"
  },
  "keywords": [
    "date",
    "time",
    "jquery-date"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/weijhfly/jqueryDatePlugin"
  },
  "author": "雾空",
  "license": "MIT",
  "dependencies": {},
